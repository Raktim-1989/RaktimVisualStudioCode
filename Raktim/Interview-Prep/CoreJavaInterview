int[] a1 = {1,2,3,4};
int[] a2 = {1,2,,3,4};

boolean flag = Arrays.equals(a1, a2);
Assert.assertTrue(flag);

Another approach:
boolean flag = true;
if(a1.length == a2.length)
{
     for(int i = 0; i<a1.length;i++)
            {
                 if(a1[i] != a2[i])
                     {
                         flag = false;
                     }
            }     

}
Assert.assertTrue(flag);

*******************************
Binray search in array
int[] array = {10,20,30,40,50};

int key = 5;
int l = 0;
int h = array.length -1 ;
boolean flag = true;


while(l<=h)
{
    
    int mid = (l+h)/2;
    if(array[mid] == key)
      {
            System.out.println("element found ");
            flag = false;
            break;
      }

    if(array[mid] < key)
     {
           l = mid + 1;
     }
     
     if(array[mid] > key)
     {
         h = mid -1 ;
     }
  
}

if(flag == true)
 {
    System.out.println();
}
